employees
-
emp_no INTEGER PK FK >- dept_manager.emp_no
birth_date DATE
first_name VARCHAR
last_name VARCHAR
gender VARCHAR
hire_date DATE

departments
-
dept_no VARCHAR
dept_name VARCHAR


dept_manager
-
dept_no VARCHAR FK >- departments.dept_no
emp_no INTEGER 
from_date DATE
to_date DATE

dept_emp
-
emp_no INTEGER PK FK >- employees.emp_no
dept_no VARCHAR FK >- departments.dept_no
from_date DATE
to_date DATE

salaries
-
emp_no INTEGER PK FK >- employees.emp_no
salary INTEGER PK
from_date DATE
to_date DATE

titles
-
emp_no INTEGER PK FK >- employees.emp_no
title VARCHAR PK
from_date DATE
to_date DATE